use_frameworks!

target 'iOS' do
    platform :ios, '10.0'
    use_frameworks!

    pod 'DZNEmptyDataSet'
    pod 'SwiftyUserDefaults'
    pod 'ProcedureKit/All', :path => 'submodules/ProcedureKit'
    pod 'ProcedureKit/Mobile', :path => 'submodules/ProcedureKit'
end

target 'macOS' do
    platform :osx, '10.10'
    use_frameworks!
    
    pod 'SwiftyUserDefaults'
end
